const prompt = require('prompt-sync')();

let edad = parseInt(prompt("Ingrese su edad: "));

const clasificar= (edad) => {
    if(edad <= 12){
        console.log("NiÃ±x");
    } else if (edad >= 13 && edad <=17){
        console.log("Adolescente");
    } else if (edad >18 && edad <= 65) {
        console.log("Adultx");
    } else {
        console.log("Ancianx");
    }
};

console.log(clasificar(edad));